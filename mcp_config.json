{
  "mcpServers": {
    "rl-a2a": {
      "command": "python",
      "args": ["mcp_server.py"],
      "env": {
        "A2A_SERVER_URL": "http://localhost:8000"
      }
    }
  },
  "server": {
    "name": "rl-a2a-mcp-server",
    "version": "1.0.0",
    "description": "MCP server for RL Agent-to-Agent Protocol",
    "homepage": "https://github.com/KunjShah01/RL-A2A"
  },
  "tools": [
    {
      "name": "register_agent",
      "description": "Register a new agent with A2A system"
    },
    {
      "name": "send_agent_feedback", 
      "description": "Send RL feedback for agent actions"
    },
    {
      "name": "create_agent_observation",
      "description": "Create agent observation messages"
    },
    {
      "name": "start_a2a_server",
      "description": "Start A2A communication server"
    }
  ],
  "resources": [
    {
      "uri": "a2a://agents",
      "description": "Active agents information"
    },
    {
      "uri": "a2a://models",
      "description": "RL models data"
    },
    {
      "uri": "a2a://communication", 
      "description": "Agent communication logs"
    }
  ]
}