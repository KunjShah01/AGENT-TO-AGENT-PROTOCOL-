{
  "version": 2,
  "builds": [
    {
      "src": "a2a_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/a2a_server.py"
    },
    {
      "src": "/ws",
      "dest": "/a2a_server.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "a2a_server.py": {
      "runtime": "python3.11"
    }
  }
}